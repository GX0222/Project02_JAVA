package tw.labs.lab01;

public class class35 {

	public static void main(String[] args) {
		Bird bird = new Bird();
		try {
			bird.setLeg(5);			
		} catch (Exception e) {
			System.out.println("ERROR");
		}
	}

}

class Bird {
	int leg;

	void setLeg(int num) throws Exception {
		if (num >= 0 && num <= 2) {
			leg = num;
		}else {
			
		}
	}
}
