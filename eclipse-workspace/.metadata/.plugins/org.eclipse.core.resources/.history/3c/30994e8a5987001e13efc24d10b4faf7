package tw.labs.lab01;

public class lab_Poker_v1 {

	public static void main(String[] args) {
		// 洗牌
		int[] poker = new int[52];
		for (int i = 0; i < poker.length; i++) {
			int temp = (int) (Math.random());
			
			for (int k = 0;k<i;k++) {
				if (temp == poker[k]) {
					break;
				}
				else {
					poker[i] = temp;
				}
			}
			
			System.out.println(poker[i]);
		}

	}

}
