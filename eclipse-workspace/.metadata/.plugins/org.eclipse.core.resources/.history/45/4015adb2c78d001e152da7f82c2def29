package tw.labs.lab01;

public class class51 {

	public static void main(String[] args) {
		

	}

}

class Class511 extends Thread{
	@Override
	public void run() {
		for (int i=0;i<100;i++) {
			System.out.println(i);
			try {
				Thread.sleep(100);
			} catch (InterruptedException e) {
				
			}
		}
	}
}

class Class512 extends Thread{
	@Override
	public void run() {
		for (int i=0;i<10;i++) {
			System.out.println(i);
			try {
				Thread.sleep(100);
			} catch (InterruptedException e) {
				
			}
		}
	}
}