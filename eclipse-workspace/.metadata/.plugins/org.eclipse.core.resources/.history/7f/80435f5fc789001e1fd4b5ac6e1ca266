package tw.lab.classes;

public class Try01Class {

	public String creatID(boolean Male) {
		String newID;
		String area = "ABCDEFGHJKLMNPQRSTUVXYWZIO";

		int areaNum = (int) (Math.random() * 26 + 1);
		newID = area.substring(areaNum, areaNum + 1);

		StringBuilder sb = new StringBuilder(newID);
		sb.append(Male ? 1 : 2);
		
		for (int i=0;i<8;i++) {
			sb.append((int)(Math.random()*9+1));
		}

		return newID;
	}
}
