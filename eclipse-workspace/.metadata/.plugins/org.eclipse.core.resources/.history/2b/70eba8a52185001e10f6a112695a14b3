package tw.labs.lab01;

public class class07 {

	public static void main(String[] args) {
		// loop
		int a = 0;
		for (; a <= 10; ) {
			System.out.println(a++);
		}

	}
	
	static void printText() {
		System.out.println("Text");
	}
	
	static void printLine() {
		System.out.println("--------");
	}

}
