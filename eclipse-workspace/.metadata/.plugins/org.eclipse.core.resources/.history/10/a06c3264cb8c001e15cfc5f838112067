package tw.labs.lab01;

public class class34 {

	public static void main(String[] args) {
		int a = -10, b = 0;
		System.out.println("Brad");
		System.out.println("-----");
		try {
			System.out.println(a / b);
		}catch(ArithmeticException e) {
			System.out.println("Oops!");
		}
		System.out.println("Finish");
	}

}
